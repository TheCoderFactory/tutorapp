<p id="notice"><%= notice %></p>

<h1>Listing Year Levels</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @year_levels.each do |year_level| %>
      <tr>
        <td><%= year_level.name %></td>
        <td><%= year_level.description %></td>
        <td><%= link_to 'Show', year_level %></td>
        <td><%= link_to 'Edit', edit_year_level_path(year_level) %></td>
        <td><%= link_to 'Destroy', year_level, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Year level', new_year_level_path %>
